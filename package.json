    "element-plus": "^2.0.2",
    "happy-dom": "^2.41.0",
    "husky": "^7.0.4",
    "js-cookie": "^3.0.1",
    "lodash.debounce": "^4.0.8",
    "pinia": "^2.0.11",
    "standard-version": "^9.3.2",
    "unplugin-auto-import": "^0.6.0",
    "unplugin-vue-components": "^0.17.18",
    "vite-plugin-windicss": "^1.7.1",
    "vue": "^3.2.31",
    "vue-i18n": "^9.2.0-beta.26",
    "vue-router": "^4.0.12",
    "windicss": "^3.4.3"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.0",
